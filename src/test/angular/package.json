  "scripts": {
    "debug": "testcafe $NODE_DEBUG_OPTION chrome",
    "test:default": "testcafe chrome \"src/*.spec.ts\"",
    "test:localhost8080": "E2E_TEST_URL=http://localhost:8080 testcafe chrome \"src/*.spec.ts\"",
    "test:chrome":  "E2E_TEST_URL=http://server:8081/?lang=de DEBUG=true testcafe --page-load-timeout 10000 --selector-timeout=10000 \"browserstack:chrome@109.0:Windows 10\" \"src/*.spec.ts\"",
    "test:firefox": "E2E_TEST_URL=http://server:8081/?lang=de DEBUG=true testcafe --page-load-timeout 10000 --selector-timeout=10000 \"browserstack:firefox@109.0:Windows 10\" \"src/*.spec.ts\"",
    "test:safari":  "E2E_TEST_URL=http://server:8081/?lang=de DEBUG=true testcafe --page-load-timeout 10000 --selector-timeout=10000 \"browserstack:safari@15.6:OS X Monterey\" \"src/*.spec.ts\""
  },
  "private": true,
  "dependencies": {
    "testcafe": "~2.3.0",
    "testcafe-browser-provider-browserstack": "^1.14.0-alpha.1",
    "ts-node": "~10.8.0",
    "typescript": "~4.7.2"
  }
